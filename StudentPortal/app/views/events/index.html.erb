<%= link_to 'New event', new_event_path %>
<br/>
<br/>
	<table>
		<tr>
			<th>Event</th>
			<th>StartTime</th>
			<th>EndTime</th>
			<th>Food</th>
			<th>Swag</th>
			<th>Comp</th>
		</tr>
		<% @events.each do |event| %>
		<tr>
			<td><%= event.title %></td>
			<td><%= event.start.to_formatted_s(:long) %></td>
			<td><%= event.end.to_formatted_s(:long) %></td>
			<td><%= event.food %></td>
			<td><%= event.swag %></td>
			<td><%= event.comp %></td>
			<td><%= link_to 'Show', event %></td>
			<td><%= link_to 'Edit', edit_event_path(event) %></td>
			<td><%= link_to 'Destroy', event_path(event), method: :delete, data: {confirm: 'Are you sure?'} %></td>
		</tr>
		<% end %>
</table>

