<div class = "aProject">
	<%= form_tag sessions_path do %>  
	  <p>  
	    <%= label_tag :userName %><br />  
	    <%= text_field_tag :userName , params[:userName ] %>  
	  </p>  
	  <p>  
	    <%= label_tag :password %><br />  
	    <%= password_field_tag :password %>  
	  </p>  
	  <p>
	  	<%= submit_tag "sign in", :class => 'button' %>
	  </p>  
	<% end %>
</div>